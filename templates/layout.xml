	<!--t-layout.php-->
	<t:layout>
		<p:xdocument>
			<p:page/>
		</p:xdocument>
    </t:layout>

	<t:whitespace-prefetch>
		<b:whitespaces-prefetch/>
	</t:whitespace-prefetch>

	<t:layout_with-overlay>
		<p:xdocument>
			<p:page/>
			<p:overlay/>
		</p:xdocument>
    </t:layout_with-overlay>

	<t:page>
		<b:page>
			<e:in>
				<p:top-header/>
				<p:header-stripe/>
				<p:main-content/>
				<p:footer/>
				<p:float-edge/>
			</e:in>
		</b:page>
	</t:page>

	<t:top-header>
		<b:stripe>
			<m:type val="overhang"/>
			<m:overhang val="top-header"/>
			<m:content val="top-header"/>
			<e:in>
				<a:columns-layout>
					<m:scheme val="abc-d"/>
					</a:columns-layout>
				<b:column>
					<m:layout val="a"/>
					<m:content val="select-region-and-top-links"/>
					<e:in>
						<p:country-select/>
						<p:top-links/>
						<!--<b:content-spacer/>-->
					</e:in>
				</b:column>
				<b:column>
					<m:layout val="b"/>
					<m:content val="welcome"/>
					<e:in>

						<p:top-auth/>

					</e:in>
				</b:column>
			</e:in>
		</b:stripe>
	</t:top-header>

	<t:header-stripe>
		<b:stripe>
			<m:type val="overhang"/>
			<m:overhang val="header"/>
			<m:content val="header-outer"/>
			<e:in>
				<p:header-content/>
				<p:stripe_content_main-menu/>
			</e:in>
		</b:stripe>
	</t:header-stripe>

	<t:header-content>
		<b:stripe>
			<m:content val="header"/>
			<e:in>
				<a:columns-layout>
					<m:scheme val="abc-d"/>
					</a:columns-layout>
				<b:interlayer>
					<m:column val="b"/>
					<b:column>
						<m:layout val="a"/>
						<e:in>

							<b:content-columns>
								<m:layout-scheme val="a-b"/>
								<m:content val="header"/>
								<b:content-column>
									<m:layout val="a"/>
										<p:sl-logo/>
										<p:slogan/>
										<p:header-promo/>
								</b:content-column>
								<b:content-column>
									<m:layout val="b"/>

									<p:content-columns_content_header-contacts/>

								</b:content-column>
							</b:content-columns>

						</e:in>
					</b:column>
					<b:column>
						<m:layout val="b"/>
						<m:content val="header-search"/>
						<e:in>
							<p:header-search/>
						</e:in>
					</b:column>
				</b:interlayer>
			</e:in>
		</b:stripe>
	</t:header-content>

	<t:content-columns_content_header-contacts>
		<b:content-columns>
			<m:layout-scheme val="a-b"/>
			<m:layout-type val="diff-dirs"/>
			<b:content-column>
				<m:layout val="a"/>
				<m:content val="region-select"/>
				<p:region-select/>
			</b:content-column>
			<b:content-column>
				<m:layout val="b"/>
				<m:content val="header-contacts"/>
				<p:header-contacts/>
			</b:content-column>
		</b:content-columns>
	</t:content-columns_content_header-contacts>

	<t:stripe_content_page-title>
		<b:stripe>
			<m:content val="page-title"/>
			<e:in>
				<m:type val="content"/>
				<p:content/>
			</e:in>
		</b:stripe>
	</t:stripe_content_page-title>

	<t:stripe_content_main-menu>
		<b:stripe>
			<m:content val="main-menu"/>
			<e:in>
				<m:type val="content"/>
				<p:menu_type_main/>
			</e:in>
		</b:stripe>
	</t:stripe_content_main-menu>

	<t:stripe_content_search><p:search-stripe/></t:stripe_content_search>
	<t:stripe_content_breadcrumbs><p:breadcrumbs-stripe/></t:stripe_content_breadcrumbs>

	<t:breadcrumbs-stripe>
		<p:content-stripe>
			<p:breadcrumbs/>
		</p:content-stripe>
	</t:breadcrumbs-stripe>

	<t:stripe_content_search_type_index>
		<b:stripe>
			<m:content val="search"/>
			<e:in>
				<a:columns-layout>
					<m:scheme val="abc-d"/>
					</a:columns-layout>
				<b:column>
					<m:layout val="a"/>
					<!--<m:content val="search-field"/>-->
					<e:in block="column">
							<p:header-search/>
					</e:in>
				</b:column>
				<b:column>
					<m:layout val="b"/>
					<!--<m:content val="search-example"/>-->
					<e:in block="column">
						<b:form-field-remark>
							<m:content val="search-example"/>
							<m:context val="full-search-columns"/>
							<cdata><![CDATA[&#8594; Лидер запросов]]></cdata>
							<b:search-example>
								<m:type val="leader"/>
								<a:link>
									<!--<m:type val="pseudo"/>-->
									</a:link>
								Microsoft
							</b:search-example>
						</b:form-field-remark>
					</e:in>
				</b:column>
			</e:in>
		</b:stripe>
	</t:stripe_content_search_type_index>


	<t:footer>
		<b:stripe>
			<m:type val="overhang"/>
			<m:overhang val="footer"/>
			<m:content val="footer"/>
			<e:in>
				<m:content val="footer"/>

				<b:stripe>
					<m:content val="footer-contacts"/>
					<e:in>
						<a:columns-layout>
							<m:type val="inner"/>
							<m:scheme val="abc-d"/>
							</a:columns-layout>

						<b:column>
							<m:layout val="a"/>
							<m:content val="footer-contacts-1"/>
							<e:in>

								<p:footer-contacts_1/>


							</e:in>
						</b:column>

						<b:column>
							<m:layout val="b"/>
							<m:content val="footer-contacts-2"/>
							<e:in>

								<p:footer-contacts_2/>


							</e:in>
						</b:column>

					</e:in>
				</b:stripe>
				<b:footer-separator><a:billet block="true"><m:content val="footer-separator"/></a:billet><e:in/></b:footer-separator>
				<b:stripe>
					<m:content val="footer-menu"/>
					<e:in>
						<a:columns-layout>
							<m:scheme val="abc-d"/>
							</a:columns-layout>
						<b:column>
							<m:layout val="a"/>
							<m:content val="footer-menu"/>
							<e:in>

								<p:footer-menu/>

							</e:in>
						</b:column>
						<b:column>
							<m:layout val="b"/>
							<m:content val="copyright"/>
							<e:in>

								<p:site-age-rating/>
								<p:copyright/>

							</e:in>
						</b:column>
					</e:in>
				</b:stripe>
			</e:in>
		</b:stripe>
	</t:footer>

	<t:main-content>
		<p:stripe_content_breadcrumbs/>
		<p:main-content__abc-d-columns/>
		<p:stripe-spacer/>
		<p:stripe_content_partners-statuses/>
	</t:main-content>

	<t:main>
		<p:page-title/>
		<p:content/>
	</t:main>

	<!--<t:main-content>-->
		<!--<p:main-content__a-columns/>-->
		<!--<p:main-content__a-b-columns/>-->
		<!--<p:main-content__a-b-c-d-columns/>-->
	<!--</t:main-content>-->

	<t:main-content__a-columns>
		<b:stripe>
			<m:content val="main"/>
			<e:in>
				<m:type val="content"/>
				<p:main/>
			</e:in>
		</b:stripe>
	</t:main-content__a-columns>

	<t:main-content__abc-d-columns>
		<b:stripe>
			<m:content val="main-sidebar"/>
			<e:in>
				<a:columns-layout>
					<m:scheme val="abc-d"/>
					</a:columns-layout>
				<p:column_main/>
				<p:column_sidebar/>
			</e:in>
		</b:stripe>
	</t:main-content__abc-d-columns>

	<t:main-content__a-c-columns>
		<b:stripe>
		<m:content val="main"/>
		<m:type val="outer"/>
		<a:form_custom-select />
			<e:in>
			<m:type val="interlayer-a-c"/>
				<b:stripe>
				<m:content val="sidebar-main-add-sidebar"/>
					<e:in>
					<a:columns-layout>
						<m:scheme val="a-c"/>
						</a:columns-layout>
						<p:column_main/>
						<p:column_add-sidebar/>
					</e:in>
				</b:stripe>
			</e:in>
		</b:stripe>
	</t:main-content__a-c-columns>

	<t:main-content__a-b-c-columns>
		<b:stripe>
		<m:content val="main"/>
		<m:type val="outer"/>
		<a:form_custom-select />
			<e:in>
			<m:type val="interlayer-a-b-c"/>
				<b:stripe>
				<m:content val="sidebar-main-add-sidebar"/>
					<e:in>
					<a:columns-layout>
						<m:scheme val="a-b-c"/>
						</a:columns-layout>
						<p:column_main/>
						<p:column_sidebar/>
						<p:column_add-sidebar/>
					</e:in>
				</b:stripe>
			</e:in>
		</b:stripe>
	</t:main-content__a-b-c-columns>

	<t:column_add-sidebar>
		<b:column>
		<m:layout val="c"/>
		<m:content val="add-sidebar"/>
			<e:in>
				<p:add-menu/>
				<p:content/>
			</e:in>
		</b:column>
	</t:column_add-sidebar>

	<t:column_sidebar>
		<b:column>
		<m:layout val="b"/>
		<m:content val="sidebar"/>
			<e:in>
				<p:section_content_menu/>
				<p:content/>
			</e:in>
		</b:column>
	</t:column_sidebar>

	<t:column_main>
		<b:column>
		<m:layout val="a"/>
		<m:content val="main"/>
			<e:in>
				<p:main/>
			</e:in>
		</b:column>
	</t:column_main>

	<t:content-stripe>
		<b:stripe>
			<e:in>
				<m:type val="content"/>
				<p:content/>
			</e:in>
		</b:stripe>
	</t:content-stripe>

	<t:stripe_type_content>
		<b:stripe>
			<e:in>
				<m:type val="content"/>
				<p:content/>
			</e:in>
		</b:stripe>
	</t:stripe_type_content>

	<t:stripe>
		<b:stripe>
			<e:in>
				<p:content/>
			</e:in>
		</b:stripe>
	</t:stripe>

	<t:stripe-spacer>
		<p:stripe_type_content>
			<b:box><m:content val="stripe-spacer"/></b:box>
		</p:stripe_type_content>
	</t:stripe-spacer>


	<!-- layout < -->

</templates>